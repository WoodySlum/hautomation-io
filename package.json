{
    "name": "hautomation-core",
    "version": "0.0.40",
    "description": "Hautomation core module",
    "main": "HautomationRunner.js",
    "engines": {
        "node": "10.14.1"
    },
    "engineStrict": true,
    "nyc": {
        "exclude": [
            "**/*Form.js",
            "**/forms/*",
            "**/logger/*",
            "**/internal-plugins/*-sensor/*",
            "**/modules/pluginsmanager/publicapis/*",
            "**/modules/camerasmanager/*",
            "**/internal-plugins/rflink/*",
            "**/internal-plugins/openweather/*",
            "**/internal-plugins/esp*",
            "**/internal-plugins/trash-reminder*",
            "**/internal-plugins/sms*",
            "**/internal-plugins/homebridge/*",
            "**/internal-plugins/tplink-tl-mr6400/*",
            "**/modules/logmanager/*",
            "**/modules/backupmanager/*",
            "**/internal-plugins/huawei*",
            "**/internal-plugins/hue/*",
            "**/internal-plugins/dropbox/*",
            "**/internal-plugins/tuya-device/*"
        ],
        "ignore-class-method": "stringify",
        "check-coverage": true,
        "per-file": true,
        "cache": true,
        "all": true,
        "temp-directory": "/tmp/cov",
        "lines": 0,
        "statements": 0,
        "functions": 0,
        "branches": 0,
        "reporter": [
            "text-summary"
        ]
    },
    "scripts": {
        "dev": "nodemon --config .nodemon.json  HautomationRunner.js",
        "start": "node HautomationRunner.js",
        "pre-test": "rm -Rf /tmp/data && mkdir /tmp/data && cp -f ./data/config.json.default /tmp/data/config.json",
        "test": "npm run pre-test && env TEST=1 NO_LOG=1 mocha --timeout 10000 --exit --reporter spec $(find tests -name '*.tests.js')",
        "cov": "env COV=1 TEST=1 NO_LOG=1 nyc npm run test",
        "lint": "eslint ./src/ --ext .js",
        "doc": "documentation build ./src/** -f md -o ./doc/DOCUMENTATION.md && documentation build ./src/modules/pluginsmanager/publicapis/** -f md -o ./doc/PUBLIC-APIS.md",
        "pre-build": "npm run doc && rm -Rf ./build/* && mkdir -p ./build/plugins && mkdir -p ./build/doc && mkdir -p ./build/data && cp ./data/config.json.default ./build/data/config.json && cp -R ./data/ssl ./build/data/ && cp ./doc/PLUGIN.md ./build/doc/ && cp ./doc/PUBLIC-APIS.md ./build/doc/ && cp -R ./scripts ./build/ && cp -R ./res ./build/",
        "build": "npm run pre-build && pkg -o ./build/hautomation . && find ./node_modules -name '*.node' -exec cp -Rf {} ./build/ \\;",
        "build-arm": "npm run pre-build && pkg -o ./build/hautomation . && find ./node_modules -name '*.node' -exec cp -Rf {} ./build/ \\; && find ./node_modules -name '*.node' | grep arm | xargs cp -Rf -t ./build/",
        "build-deb": "rm -Rf ./build/deb && rm -Rf /tmp/deb && cp -R ./scripts/deb /tmp/deb && sed -e s@{VERSION}@$npm_package_version@ ./scripts/deb/DEBIAN/control > /tmp/deb/DEBIAN/control && cp -R ./build/* /tmp/deb/var/hautomation/ && chmod 755 /tmp/deb/DEBIAN/pre* && chmod 755 /tmp/deb/DEBIAN/post* && dpkg-deb --build /tmp/deb ./build/hautomation.deb && rm -Rf /tmp/deb"
    },
    "bin": "HautomationRunner.js",
    "pkg": {
        "scripts": [
            "./src/**/*.js"
        ],
        "assets": [
            "./node_modules/ngrok/bin/ngrok",
            "./node_modules/threads/lib/worker.node/slave.js",
            "./**/*.json",
            "./src/**/*.c",
            "./src/**/*.cpp",
            "./src/**/*.hpp",
            "./src/**/*.h",
            "./src/**/lib/*",
            "./src/**/global_lib/*",
            "./**/*.py",
            "./src/modules/botengine/linux/asoundrc"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git@honey.hd.free.fr:hautomation/core-node.git"
    },
    "keywords": [
        "hautomation",
        "core"
    ],
    "author": "WoodySlum",
    "license": "ISC",
    "devDependencies": {
        "chai": "^4.2.0",
        "parse-domain": "2.1.7",
        "documentation": "^5.5",
        "eslint": "^3.19",
        "eslint-plugin-jsdoc": "^3.9.1",
        "eslint-plugin-mocha": "^4.12",
        "eslint-plugin-require-jsdoc": "^1.0",
        "mocha": "^5.2",
        "mocha-junit-reporter": "^1.18.0",
        "node-pre-gyp": "latest",
        "nodemon": "^1.18.7",
        "nyc": "^12.0",
        "pkg": "4.3.8",
        "remove-console-logs": "^0.1",
        "resolve-options": "^1.1",
        "sinon": "^7.2",
        "nan": "^2.11"
    },
    "dependencies": {
        "@bokub/linky": "latest",
        "annotation": "git://github.com/WoodySlum/node-annotation.git#master",
        "audiohub": "git://github.com/WoodySlum/audiohub.git#master",
        "body-parser": "^1.18",
        "callsite": "^1.0",
        "cloudtuya": "git://github.com/unparagoned/cloudtuya#master",
        "color-util": "^2.2",
        "columnify": "^1.5",
        "compression": "^1.7",
        "dialog-router-api": "git://github.com/WoodySlum/dialog-router-api.git#master",
        "dropbox": "^4.0",
        "exec-extra": "^2.4.1",
        "express": "^4.16",
        "extract-zip": "^1.6",
        "fs-extra": "^4.0",
        "fs-readdir-recursive": "^1.1.0",
        "gm": "^1.23.1",
        "homebridge": "^0.4",
        "homebridge-alexa": "^0.4",
        "huejay": "^1.8",
        "is-running": "^2.1",
        "isomorphic-fetch": "^2.2",
        "mac-scanner": "^1.1",
        "md5-file": "^3.2",
        "mjpeg-proxy": "^0.2",
        "moment": "^2.22",
        "moment-timezone": "^0.5",
        "net-ping": "^1.2",
        "ngrok": "^3.1",
        "node-datetime": "^1.0",
        "node-google-timezone": "^0.1",
        "node-gtts": "^2.0",
        "node-gyp": "latest",
        "node-record-lpcm16": "^0.3",
        "node-wit": "^4.3",
        "node-witai-speech": "^1.0",
        "prowler": "^0.0",
        "qrcode": "^1.3",
        "remi": "^2.0",
        "remi-runner": "^1.1",
        "serialport": "6.2.2",
        "sha256": "^0.2",
        "sqlite3": "git://github.com/WoodySlum/node-sqlite3.git#master",
        "stack-trace": "^0.0",
        "string-similarity": "^1.2",
        "threads": "git://github.com/WoodySlum/threads.js.git#master",
        "toposort": "^1.0",
        "underscore": "^1.9",
        "usb-detection": "^4.1",
        "waveheader": "^0.0",
        "xml2js": "^0.4",
        "xmlhttprequest-ssl": "^1.5",
        "zip-dir": "^1.0"
    }
}
